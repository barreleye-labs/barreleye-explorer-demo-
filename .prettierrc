{
  "printWidth": 120,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "none",
  "semi": true,
  "importOrder": [
    "react",
    "<THIRD_PARTY_MODULES>",
    "^@services/(.*)$",
    "^@type/(.*.*)$",
    "^@hooks/(.*)$",
    "^@layouts/(.*)$",
    "^@pages/(.*)$",
    "^@components/(.*)$",
    "^@utils",
    "^@config/(.*)$",
    "^@styles/(.*)$",
    "^@src/(.*)$",
    "^[./]"
  ],
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}